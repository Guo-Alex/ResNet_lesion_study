net: "CNN_bn_40.prototxt"
snapshot_prefix: "snapshot/CNN_bn_40"
# snapshot intermediate results
snapshot: 10000

#test_iter * batch_size = test_num(50W)
#test_iter: 10000
#test once every test_interval 
#test_interval: 5000
base_lr: 0.001
lr_policy: "step"
stepsize: 5000
gamma: 0.9
display: 1
max_iter: 500001
momentum: 0.9
weight_decay: 0.0005
solver_mode: GPU
#use_polyak: true
#num_iter_polyak: 256
